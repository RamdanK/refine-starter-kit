  "scripts": {
    "dev": "refine dev",
    "build": "tsc && refine build",
    "preview": "refine start",
    "refine": "refine",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write */**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "lint:strict": "eslint '*/**/*.{js,jsx,ts,tsx}'",
